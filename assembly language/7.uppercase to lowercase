;uppercase to lowercase

.model small
.stack 100h
.data
  letter_1 db '$'
  letter_2 db 0dh,0ah,'$'
.code
 main proc
    mov ax,@data
    mov ds,ax
    
    lea dx,letter_1
    mov ah,9
    int 21h
    
    mov ah,1
    int 21h
    
    mov bl,al
    
    lea dx,letter_2
    mov ah,9
    int 21h
    add bl,32
    
    mov ah,2
    mov dl,bl
    int 21h
    
    mov ah,4ch
    int 21h
    main endp
    end main
